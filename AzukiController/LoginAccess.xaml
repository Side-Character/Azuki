        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AzukiController"
        mc:Ignorable="d"
        Title="LoginAccess" Height="163" Width="199" ResizeMode="NoResize">
    <Grid>
        <TextBox Margin="10,33,0,0" Text="Username" TextWrapping="Wrap" HorizontalContentAlignment="Center" Width="175" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <PasswordBox HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="175"/>
    </Grid>
</Window>
